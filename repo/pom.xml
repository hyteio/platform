						<phase>package</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<copyPom>true</copyPom>
							<excludeTransitive>true</excludeTransitive>
							<excludeArtifactIds>apache-karaf,pax-logging-log4j2-extra</excludeArtifactIds>
							<outputDirectory>${project.build.directory}/local-repo</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
							<useRepositoryLayout>true</useRepositoryLayout>
						</configuration>
					</execution>
					<execution>
						<id>copy.system</id>
						<phase>package</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<copyPom>true</copyPom>
							<excludeTransitive>true</excludeTransitive>
							<includeArtifactIds>pax-logging-log4j2-extra</includeArtifactIds>
							<outputDirectory>${project.build.directory}/hyte-system</outputDirectory>
							<overWriteRelease>false</overWriteRelease>
							<overWriteSnapshots>true</overWriteSnapshots>
							<useRepositoryLayout>true</useRepositoryLayout>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<executions>
					<execution>
						<id>hyte-platform-repo</id>
						<phase>package</phase>
						<goals>
